{
    "name": "barryvdh/laravel-elfinder",
    "keywords": [
        "laravel",
        "elfinder",
        "tinymce",
        "filebrowser"
    ],
    "description": "A Laravel Package to integrate elFinder 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:media253/elFinder"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "studio-42/elfinder": "dev-master",
        "barryvdh/elfinder-flysystem-driver": "^0.4.2"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\Elfinder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        },
        "laravel": {
            "providers": [
                "Barryvdh\\Elfinder\\ElfinderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
